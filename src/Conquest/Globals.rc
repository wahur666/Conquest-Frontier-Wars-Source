IDD_DIALOG1 DIALOG  0, 0, 189, 109
STYLE DS_SETFONT | DS_MODALFRAME | WS_POPUP | WS_CAPTION | WS_SYSMENU
CAPTION "Adjust Network Stress"
FONT 10, "MS Sans Serif"
BEGIN
    LTEXT           "Latency (in mseconds)",IDC_STATIC,6,9,68,9
    EDITTEXT        IDC_EDIT1,83,7,42,12,ES_AUTOHSCROLL | ES_NUMBER
    CONTROL         "Spin1",IDC_SPIN1,"msctls_updown32",UDS_SETBUDDYINT | 
                    UDS_ALIGNRIGHT | UDS_AUTOBUDDY | UDS_ARROWKEYS | 
                    UDS_NOTHOUSANDS,126,7,8,13
    GROUPBOX        "",IDC_STATIC,1,0,187,55
    LTEXT           "The default latency value is 0. This means that messages are handled immediately upon receipt. Setting a higher value causes messages to wait in the input queue X amount of time before the game processes them.",
                    IDC_STATIC,5,21,179,32
    GROUPBOX        "",IDC_STATIC,1,56,186,50
    LTEXT           "Packet Loss (percent)",IDC_STATIC,6,65,66,9
    EDITTEXT        IDC_EDIT2,83,63,42,12,ES_AUTOHSCROLL | ES_NUMBER
    CONTROL         "Spin1",IDC_SPIN2,"msctls_updown32",UDS_SETBUDDYINT | 
                    UDS_ALIGNRIGHT | UDS_AUTOBUDDY | UDS_ARROWKEYS | 
                    UDS_NOTHOUSANDS,126,63,8,13
    LTEXT           "The default packet loss is 0. This means that all messages get through to the destination. Setting a higher value causes more packets to be lost in transit.",
                    IDC_STATIC,6,79,171,26
END

